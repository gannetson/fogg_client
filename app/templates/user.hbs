{{google-map allCountries=allCountries countries=model.countries countriesNeeded=countriesNeeded}}

<div id="legend">
    <h3>{{model.countries.length}} countries visited</h3>
    <ul class="countries">
        {{#each model.countries as |country|}}
            <li>
                <span class="spaced icon-globe"></span>
                <span class="name">{{country.name}}</span>
            </li>
        {{/each}}
    </ul>
</div>

